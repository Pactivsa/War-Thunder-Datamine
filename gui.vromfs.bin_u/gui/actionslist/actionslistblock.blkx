{
  "id": "actions_list",
  "menu_align": "",
  "not-input-transparent": "yes",
  "iconed": "no",
  "tooltip": "",
  "on_end_edit": "onActionsListDeactivate",
  "flow": "vertical",
  "flow-align": "center",
  "listStyle": "center",
  "min-width": "0",
  "background-color": "@actionsListBgColor",
  "css-hier-invalidate": "yes",
  "hasActions": "yes",
  "move_out": "no",
  "action_list_info": {
    "id": "info_block",
    "inactive": "yes",
    "interactive": "no",
    "position": "relative",
    "left": "(pw-w)/2",
    "width": "pw",
    "css-hier-invalidate": "yes"
  },
  "tdiv": {
    "id": "info_block_separator",
    "position": "relative",
    "pos": "(pw-w)/2, 1@sf/@pf",
    "display": "hide",
    "background-color": "@menuSeparatorColor",
    "inactive": "yes"
  },
  "itemsList": {
    "id": "list_nest",
    "flow": "vertical",
    "min-width": "pw",
    "behavior": "posNavigator",
    "navigatorShortcuts": "cancel",
    "on_set_focus": "onFocus",
    "on_cancel_edit": "onBtnClose"
  },
  "timer": {
    "id": "update_timer",
    "timer_handler_func": "update",
    "timer_interval_msec": "100"
  }
}