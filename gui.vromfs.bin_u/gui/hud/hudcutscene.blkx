{
  "tdiv": [
    {
      "id": "hud_hints_nest",
      "size": "pw, ph",
      "position": "absolute",
      "css-hier-invalidate": "yes",
      "behavior": "Timer",
      "tdiv": [
        {
          "pos": "pw/2 - w/2, 1@topCenterMessagesTopPos",
          "position": "absolute",
          "flow": "vertical",
          "tdiv": {
            "id": "hud_message_killer_card",
            "position": "relative",
            "pos": "pw/2 - w/2, -0.04@shHud"
          },
          "hudMessages": {
            "id": "hud_messages_death_reason_notification",
            "width": "@shHud",
            "pos": "pw/2 - w/2, 0.85@topCenterMainMessagesMinHeight",
            "position": "relative",
            "flow": "vertical",
            "message_style": "death_reason_notification"
          }
        },
        {
          "id": "mission_hints",
          "width": "2/3@rwHud",
          "pos": "pw-w, 0.9ph",
          "position": "relative",
          "flow": "vertical",
          "flow-align": "right"
        }
      ]
    },
    {
      "id": "hud_live_stats_nest",
      "width": "@shHud",
      "pos": "pw/2-w/2, ph-h-0.032@shHud",
      "position": "absolute",
      "display": "hide"
    },
    {
      "id": "minor_priority_hints",
      "width": "pw",
      "position": "absolute",
      "pos": "50%pw-50%w, ph - h",
      "flow": "vertical"
    },
    {
      "id": "hud_message_timers"
    }
  ],
  "timer": {
    "id": "hud_message_timer"
  }
}