{
  "flow": "horizontal",
  "profileContentBigIcon": {
    "position": "relative",
    "size": "1@itemIconBlockHeight, 1@itemIconBlockHeight",
    "img": {
      "id": "image"
    }
  },
  "tdiv": {
    "position": "relative",
    "id": "text_container",
    "flow": "vertical",
    "width": "fw",
    "margin-left": "2@blockInterval",
    "profilePageTitle": {
      "id": "header",
      "max-width": "pw",
      "hideEmptyText": "yes",
      "overlayTextColor": "unlockHeader",
      "padding-bottom": "10@sf/@pf"
    },
    "tdiv": [
      {
        "id": "conditions",
        "flow": "vertical",
        "challengeDescriptionProgress": {
          "id": "progress",
          "isProfileUnlockProgress": "yes",
          "margin-bottom": "8@sf/@pf",
          "value": "",
          "display": "hide"
        },
        "profilePageText": [
          {
            "id": "mainCond",
            "max-width": "6@decalIconHeight",
            "overflow": "hidden",
            "smallFont": "yes",
            "margin-top": "@blockInterval",
            "hideEmptyText": "yes",
            "margin-bottom": "8@sf/@pf",
            "color": "@profilePageTextColor"
          },
          {
            "id": "multDesc",
            "max-width": "6@decalIconHeight",
            "overflow": "hidden",
            "smallFont": "yes",
            "color": "@profilePageTextColor"
          },
          {
            "id": "conds",
            "max-width": "6@decalIconHeight",
            "overflow": "hidden",
            "smallFont": "yes",
            "hideEmptyText": "yes",
            "color": "@profilePageTextColor"
          },
          {
            "id": "obtain_info",
            "hideEmptyText": "yes",
            "max-width": "6@decalIconHeight",
            "smallFont": "yes",
            "color": "@profilePageTextColor"
          }
        ]
      },
      {
        "id": "additional_description",
        "behavior": "posNavigator",
        "margin-top": "8@dp",
        "flow": "h-flow",
        "width": "pw",
        "display": "hide",
        "hideEmptyText": "yes",
        "canSelectNone": "yes"
      },
      {
        "position": "relative",
        "left": "-1@buttonMargin",
        "margin-top": "8@sf/@pf",
        "Button_text": [
          {
            "id": "btn_preview",
            "display": "hide",
            "text": "#mainmenu/btnPreview",
            "btnName": "L3",
            "_on_click": "onDecoratorPreview",
            "showButtonImageOnConsole": "no",
            "class": "image",
            "visualStyle": "secondary",
            "img": {
              "background-image": "#ui/gameuiskin#btn_preview.svg"
            },
            "buttonWink": {},
            "ButtonImg": {}
          },
          {
            "id": "btn_marketplace_find_coupon",
            "text": "#msgbox/btn_find_on_marketplace",
            "btnName": "X",
            "on_click": "onBtnMarketplaceFindCoupon",
            "display": "hide",
            "showButtonImageOnConsole": "no",
            "visualStyle": "secondary",
            "class": "image",
            "buttonWink": {},
            "img": {
              "background-image": "#ui/gameuiskin#gc.svg"
            },
            "ButtonImg": {}
          },
          {
            "id": "btn_store",
            "text": "#items/shop/emptyTab/toShopButton",
            "btnName": "X",
            "on_click": "onItemsShop",
            "display": "hide",
            "showButtonImageOnConsole": "no",
            "visualStyle": "secondary",
            "class": "image",
            "buttonWink": {},
            "img": {
              "background-image": "#ui/gameuiskin#store_icon.svg"
            },
            "ButtonImg": {}
          },
          {
            "id": "btn_marketplace_consume_coupon",
            "text": "#item/consume/coupon",
            "btnName": "X",
            "on_click": "onBtnMarketplaceConsumeCoupon",
            "display": "hide",
            "showButtonImageOnConsole": "no",
            "visualStyle": "secondary",
            "class": "image",
            "buttonWink": {},
            "img": {
              "background-image": "#ui/gameuiskin#gc.svg"
            },
            "ButtonImg": {}
          },
          {
            "id": "btn_buy_decorator",
            "text": "#mainmenu/btnOrder",
            "btnName": "X",
            "_on_click": "onBuyDecorator",
            "hideText": "yes",
            "display": "hide",
            "showButtonImageOnConsole": "no",
            "visualStyle": "purchase",
            "buttonWink": {},
            "buttonGlance": {},
            "ButtonImg": {},
            "textarea": {
              "id": "btn_buy_decorator_text",
              "class": "buttonText"
            }
          }
        ]
      }
    ],
    "profilePageText": [
      {
        "id": "description",
        "margin-top": "@blockInterval",
        "hideEmptyText": "yes",
        "max-width": "6@decalIconHeight",
        "smallFont": "yes",
        "color": "@profilePageTextColor"
      },
      {
        "id": "price",
        "display": "hide",
        "max-width": "6@decalIconHeight",
        "hideEmptyText": "yes",
        "margin-top": "@blockInterval",
        "color": "@profilePageTextColor"
      }
    ]
  }
}